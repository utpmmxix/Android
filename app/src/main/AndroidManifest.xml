        <activity android:name=".Views.Interfaces.SuccessfulActivity" />
        <activity android:name=".Views.Interfaces.RecoverActivity" />
        <activity android:name=".Views.Interfaces.RegisterActivity" />
        <activity android:name=".Views.Interfaces.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>