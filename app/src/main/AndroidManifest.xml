        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Views.Interfaces.activity_conversacion"></activity>
        <activity android:name=".Views.Interfaces.activity_bandeja" />
